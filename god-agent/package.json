{
  "name": "god-agent",
  "version": "0.1.0",
  "description": "God Agent Core Memory System - Neuro-symbolic cognitive architecture for multi-agent orchestration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "god-agent": "dist/cli/index.js",
    "god-agent-mcp": "dist/mcp-server.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp src/storage/schema.sql dist/storage/schema.sql",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "mcp": "node dist/mcp-server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "ai",
    "memory",
    "vector-search",
    "hnsw",
    "provenance",
    "causal-reasoning",
    "multi-agent",
    "claude-code"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@modelcontextprotocol/sdk": "^1.25.0",
    "@playwright/test": "^1.57.0",
    "better-sqlite3": "^11.6.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "knex": "^3.1.0",
    "madge": "^8.0.0",
    "openai": "^4.73.0",
    "ora": "^8.1.0",
    "playwright": "^1.57.0",
    "simple-git": "^3.30.0",
    "source-map": "^0.7.6",
    "stack-utils": "^2.0.6",
    "ts-morph": "^27.0.2",
    "turndown": "^7.2.2",
    "typescript-language-server": "^5.1.3",
    "uuid": "^10.0.0",
    "v8-profiler-next": "^1.10.0",
    "vscode-languageserver-protocol": "^3.17.5",
    "ws": "^8.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.0",
    "@types/stack-utils": "^2.0.3",
    "@types/turndown": "^5.0.6",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.0"
  }
}
